{
  "compilerOptions": {
    "baseUrl": ".",
    "noEmit": true,
    "jsx": "react",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "paths": {
      "@components/*": ["src/components/*"]
    }
  },
  "exclude": [
    "node_modules",
    "build",
  ],
}
